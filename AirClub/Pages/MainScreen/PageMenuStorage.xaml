<Page x:Class="AirClub.Pages.MainScreen.PageMenuStorage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AirClub.Pages.MainScreen"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      DataContext="{Binding MenuViewModel, Source={StaticResource ViewModelLocator}}"
      Title="PageMenuServices">

    <Grid>
        <WrapPanel Orientation="Vertical">

            <Button Command="{Binding ShowStorages}" Margin="5" Padding="10 5" Content="Склады и аэродромы" />
            <Button Command="{Binding ShowStorageItemTypes}" Margin="5" Padding="10 5" Content="Типы предметов склада" />
            <Button Command="{Binding ShowJetModels}" Margin="5" Padding="10 5" Content="Модели самолетов" />
            <Button Command="{Binding ShowJets}" Margin="5" Padding="10 5" Content="Самолеты" />
            <Button Command="{Binding ShowStorage}" Margin="5" Padding="10 5" Content="Хранение" />
        </WrapPanel>

    </Grid>
</Page>
