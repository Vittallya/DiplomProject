<Page x:Class="AirClub.Pages.MainScreen.PageMenuTours"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:AirClub.Pages.MainScreen"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      DataContext="{Binding MenuViewModel, Source={StaticResource ViewModelLocator}}"
      Title="PageMenuServices">

    <Grid>
        <WrapPanel Orientation="Vertical">

            <Button Command="{Binding ShowTours}" Margin="5" Padding="10 5" Content="Туры" />
            <Button Command="{Binding ShowTransfers}" Margin="5" Padding="10 5" Content="Трансферы" />

        </WrapPanel>

    </Grid>
</Page>
